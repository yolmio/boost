{
  "name": "@yolm/boost",
  "version": "0.2.1",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist/**",
    "material-icons/**"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outdir=dist --format=esm --platform=node --minify --sourcemap",
    "build:watch": "esbuild src/index.ts --bundle --outdir=dist --format=esm --platform=node --watch",
    "create-ts": "tsc --project tsconfig.json",
    "create-ts:watch": "tsc --project tsconfig.json --watch",
    "lint": "TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "prettier --write \"**/*.ts\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@types/node": "=20.3.1",
    "@types/which": "^3.0.0",
    "csstype": "=3.1.2",
    "node-fetch": "=3.3.1",
    "toposort": "=2.0.2",
    "which": "^3.0.1"
  },
  "devDependencies": {
    "@types/toposort": "=2.0.3",
    "esbuild": "^0.18.18",
    "eslint": "=8.42.0",
    "fast-glob": "=3.2.12",
    "fs-extra": "=11.1.1",
    "jsdom": "=22.1.0",
    "prettier": "=2.8.8",
    "svgo": "=2.8.0",
    "typescript": "=5.1.3"
  }
}
